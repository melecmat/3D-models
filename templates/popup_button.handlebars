{{#each annotations}}
<a-text 
    geometry="primitive:plane"
    material="transparent:true; opacity:0;"
    value="{{get_number @key}}" width="6" align="center"
    look-at-camera="" {{! look-at camera property}}
    color="white" id="rendered{{@key}}"
    class="clickable rendered_annotation" info-window=" window_id:{{@key}}" position="{{position}}" data-clickable
></a-text>
{{/each}}